<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.OishiWeek_Project" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/mellow_apricot</item>
        <item name="colorPrimaryVariant">@color/sandy_brown</item>
        <item name="colorOnPrimary">@color/dark_blue_gray</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/apricot</item>
        <item name="colorSecondaryVariant">@color/tumbleweed</item>
        <item name="colorOnSecondary">@color/dark_blue_gray</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.OishiWeek_Project.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.OishiWeek_Project.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.OishiWeek_Project.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>